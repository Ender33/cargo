{
  "name": "ender33/cargo",
  "description": "The most flexible deployment tool for web apps",
  "homepage": "https://github.com/Ender33/cargo",
  "license": "MIT",
  "type": "library",
  "keywords": ["deployment"],
  "authors": [
    {
      "name": "Pierre Jacquet",
      "email": "pierre.jacquet@cws-studio.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "monolog/monolog": "^1.0",
    "symfony/console": "^3.0",
    "symfony/filesystem": "^3.0",
    "symfony/event-dispatcher": "^3.0",
    "symfony/finder": "^3.0",
    "symfony/yaml": "^3.0",
    "symfony/process": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "satooshi/php-coveralls": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Cargo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mage\\Tests\\": "tests/"
    }
  },
  "bin": ["bin/cargo"]
}